{
  "name": "@prefactor/sdk",
  "version": "0.1.3",
  "description": "Automatic observability for LangChain.js agents",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run scripts/build.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format --write .",
    "example:basic": "bun examples/basic.ts",
    "example:anthropic": "bun examples/anthropic-agent/simple-agent.ts",
    "prepublishOnly": "bun run build && bun run test && bun run typecheck"
  },
  "keywords": [
    "prefactor",
    "observability",
    "tracing",
    "langchain",
    "llm",
    "agent",
    "monitoring"
  ],
  "author": "Prefactor",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@prefactor/pfid": "^0.1.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/node": "^20.0.0",
    "bun-types": "latest",
    "langchain": "^1.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
